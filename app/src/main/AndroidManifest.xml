    xmlns:tools="http://schemas.android.com/tools"
    package="com.kor45cw.retrofitexample">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        android:name=".Retrofit.kakao_login.GlobalApplication"

        android:networkSecurityConfig="@xml/network_security_config"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning,UnusedAttribute">
        <!-- <activity android:name=".MainActivity">    -->
        <activity android:name=".Retrofit.kakao_login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Setting for Native App KEY -->
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="{cd1d69d6d32fd6eeeb46a0e9737bc4f3}" />


    </application>

</manifest>